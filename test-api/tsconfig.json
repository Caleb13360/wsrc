{
  "compilerOptions": {
    "target": "es2016",        
    "module": "Node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node16",
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "outDir": "./dist",
  },
  "exclude": ["node_modules/"],
  "include": ["src/**/*.ts"]
}
