{
  "compilerOptions": {
    "target": "es2016",        
    "module": "Node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node16",
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@models/*": ["../models/*"]
    }
  },
  "exclude": ["node_modules/"],
  "include": ["src/**/*.ts"]
}
